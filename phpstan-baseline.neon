parameters:
    ignoreErrors:
        - # https://github.com/phpstan/phpstan-phpunit/issues/87
            message: '#^Trying to mock an undefined method [a-zA-Z]*\(\) on class stdClass\.$#'
            path: tests/
        - # TODO: Remove this when doctrine/orm 2.9.3 is released and do the same for psalm.
            message: '#Doctrine\\ORM\\Mapping\\ClassMetadata\<object\>\.$#'
            path: ./
